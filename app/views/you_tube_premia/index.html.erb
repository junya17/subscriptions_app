<p id="notice"><%= notice %></p>

<h1>You Tube Premia</h1>

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Date</th>
      <th>Update period</th>
      <th>Status</th>
      <th>Memo</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @you_tube_premia.each do |you_tube_premium| %>
      <tr>
        <td><%= you_tube_premium.price %></td>
        <td><%= you_tube_premium.date %></td>
        <td><%= you_tube_premium.update_period %></td>
        <td><%= you_tube_premium.status %></td>
        <td><%= you_tube_premium.memo %></td>
        <td><%= you_tube_premium.user_id %></td>
        <td><%= link_to 'Show', you_tube_premium %></td>
        <td><%= link_to 'Edit', edit_you_tube_premium_path(you_tube_premium) %></td>
        <td><%= link_to 'Destroy', you_tube_premium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New You Tube Premium', new_you_tube_premium_path %>
