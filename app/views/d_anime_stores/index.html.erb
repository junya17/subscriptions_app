<p id="notice"><%= notice %></p>

<h1>D Anime Stores</h1>

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Date</th>
      <th>Update period</th>
      <th>Status</th>
      <th>Memo</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @d_anime_stores.each do |d_anime_store| %>
      <tr>
        <td><%= d_anime_store.price %></td>
        <td><%= d_anime_store.date %></td>
        <td><%= d_anime_store.update_period %></td>
        <td><%= d_anime_store.status %></td>
        <td><%= d_anime_store.memo %></td>
        <td><%= d_anime_store.user_id %></td>
        <td><%= link_to 'Show', d_anime_store %></td>
        <td><%= link_to 'Edit', edit_d_anime_store_path(d_anime_store) %></td>
        <td><%= link_to 'Destroy', d_anime_store, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New D Anime Store', new_d_anime_store_path %>
