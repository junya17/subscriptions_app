<p id="notice"><%= notice %></p>

<h1>Line Musics</h1>

<table>
  <thead>
    <tr>
      <th>Price</th>
      <th>Date</th>
      <th>Update period</th>
      <th>Status</th>
      <th>Memo</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @line_musics.each do |line_music| %>
      <tr>
        <td><%= line_music.price %></td>
        <td><%= line_music.date %></td>
        <td><%= line_music.update_period %></td>
        <td><%= line_music.status %></td>
        <td><%= line_music.memo %></td>
        <td><%= line_music.user_id %></td>
        <td><%= link_to 'Show', line_music %></td>
        <td><%= link_to 'Edit', edit_line_music_path(line_music) %></td>
        <td><%= link_to 'Destroy', line_music, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Line Music', new_line_music_path %>
